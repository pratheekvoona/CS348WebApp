<%- include("partials/header"); -%>
    <div class="table-wrapper">
        <% if (tot.length > 0) {%>
            <table class="table table-hovered">
                <thead class="thead-dark">
                    <tr>
                        
                        <th scope="col">Total Number of Classes that are Reviewed</th>
                                                
                                               
                                                
                    </tr>
                </thead>
                <tbody>
                    <% tot.forEach((study_len, index) => { %>
                        <tr>
                            <th scope="row"><%= study_len.num %></th>
                            
                                                
                                                        
                                                        
                        </tr>
                    <% }) %>
                </tbody>
            </table>

        <% } else { %>
            <p class="text-center">No classes found. Go <a href="/addProfessor" >here</a> to add Reviews.</p>
        <% } %>
    </div>
</div>
</body>
</html>